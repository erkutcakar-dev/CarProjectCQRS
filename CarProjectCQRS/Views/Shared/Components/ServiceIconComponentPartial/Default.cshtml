@model List<CarProjectCQRS.CQRSPattern.Results.Service.GetServiceQueryResult>

<div class="container">
    <div class="row g-4">
        @foreach (var item in Model)
        {
            <div class="col-md-4 wow fadeInUp" data-wow-delay="0.1s">
                <div class="service-item p-4">
                    <div class="service-icon mb-4">
                        <i class="@item.Icon"></i>
                    </div>
                    <h5 class="mb-3">@item.IconTitle</h5>
                    <p class="mb-0">@item.IconSubtitle</p>
                </div>
            </div>
        }
    </div>
</div>
